{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "url": "",
      "headers": {},
      "oauthScopes": ["openid", "email", "profile", "offline_access"],
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "${CONTEXT7_API_KEY}"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "timeout": 120000,
      "disabled": false
    },
    "fetch": {
      "type": "stdio",
      "url": "",
      "headers": {},
      "oauthScopes": ["openid", "email", "profile", "offline_access"],
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "timeout": 120000,
      "disabled": false
    }
  }
}
